{
  "name": "babel-plugin-tailcall-optimization",
  "version": "1.0.6",
  "description": "Tail call optimization for JavaScript!",
  "main": "build/lib.js",
  "scripts": {
    "compile": "rm -rf build && babel -d build/ src/",
    "compile:dev": "babel -s -d build/ src/",
    "prestart": "npm run compile",
    "start": "node build/lib.js",
    "pretest": "npm run compile:dev",
    "prepublish": "npm run compile",
    "lint": "standard --fix",
    "test": "npm run lint && mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krzkaczor/babel-plugin-tco.git"
  },
  "author": "Krzysztof Kaczor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krzkaczor/babel-plugin-tco/issues"
  },
  "homepage": "https://github.com/krzkaczor/babel-plugin-tco",
  "dependencies": {
    "babel-preset-es2015": "^6.13.0",
    "babel-traverse": "^6.13.0",
    "babylon": "^6.8.0",
    "babel-cli": "^6.11.0",
    "babel-core": "^6.13.0"
  },
  "devDependencies": {
    "babel-register": "6.11.6",
    "chai": "3.5.0",
    "mocha": "2.5.3",
    "standard": "7.1.2"
  },
  "standard": {
    "env": [ "mocha" ]
  }
}
